{
	"name": "frontend",
	"dockerComposeFile": "../../compose.yaml",
	"service": "frontend",
	"shutdownAction": "none",
	"workspaceFolder": "/workspace/frontend",
	"postCreateCommand": "npm install",
	"postStartCommand": "npm run dev",
	"forwardPorts": [
		3000
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"esbenp.prettier-vscode",
				"bradlc.vscode-tailwindcss"
			]
		}
	},
}